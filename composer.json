{
    "name": "ajtarragona/t-components",
    "description": "A collection of blade components for your laravel application using Livewire and alpine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Txomin Medrano Martorell",
            "email": "tmedrano@tarragona.cat"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^6.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "livewire/livewire": "^1.0",
        "laravel/helpers": "^1.0@dev"
        
    },
    "autoload": {
        "psr-4": {
            "Ajtarragona\\TComponents\\": "./src"
        },
        "files": [
            "src/Helpers/t-components.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajtarragona\\TComponents\\TComponentsServiceProvider"
            ]
        }
    }
}
